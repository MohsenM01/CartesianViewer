<UserControl x:Class="TopNavigatorModule.Views.TopNavigatorUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TopNavigatorModule.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" Margin="5" >
        <Button Command="{Binding NavigateCommand}" CommandParameter="CartesianViewer" Margin="5" Width="125">Clear</Button>
        <Button Command="{Binding NavigateCommand}" CommandParameter="LoadDataDialog" Margin="5" Width="125">Read From File</Button>
    </StackPanel>
</UserControl>
